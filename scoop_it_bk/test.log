<script>
    var recaptchaDiv = null;

    function hackFixPositionOfRecaptcha() {
        if (recaptchaDiv == null) {
            var body = document.getElementsByTagName("body")[0];
            var children = body.childNodes;
            // YOLO
            for (var i = 0; i < children.length; i++) {
                var child = children[i];
                if (child.tagName === "DIV" && child.style.top === "-10000px") {
                    recaptchaDiv = child;
                }
            }
        } else {
            if (recaptchaDiv.style.left.indexOf("-") >= 0) {
                recaptchaDiv.style.left = "0px";
            }
        }
        window.setTimeout(hackFixPositionOfRecaptcha, 2000);
    }

    hackFixPositionOfRecaptcha();
