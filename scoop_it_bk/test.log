jQuery(document).ready(function() {
    var displayNameInput = jQuery("#subscriptionForm input[name='displayName']");
    var shortNameInput = jQuery("#subscriptionForm input[name='shortName']");
    var emailInput = jQuery("#subscriptionForm input[name='email']");
    var passwordInput = jQuery("#subscriptionForm input[name='password']");
    var changeShortName = jQuery("#changeShortName");
    var profileURLBlock = jQuery('#profileURLBlock');
    var profileURL = jQuery('#profileURL');
    var shortNameEdition = jQuery('#shortNameEdition');
    var displayNameError = jQuery('#displayNameError');
    var shortNameError = jQuery('#shortNameError');
    var emailError = jQuery('#emailError');
    var passwordError = jQuery('#passwordError');

    displayNameInput.bind('keyup blur', function() {
        handleDisplayNameEdit();  
    });
    shortNameInput.bind('keyup blur', function() {
        handleShortNameEdit(true);
    });
    changeShortName.click(function() {
    	profileURLBlock.hide();
    	shortNameEdition.show();
        return false;
    });
    passwordInput.bind('blur', function() {
        handlePasswordEdit();
    });

    var g_displayNameRequestCounter = 0;
    function handleDisplayNameEdit() {
    	g_displayNameRequestCounter++;
        gjAjax.postRequest("/subscribe/ajax", {view: "json", action: "checkDisplayName", displayName: displayNameInput.val(), urlizedDisplayName: urlize(displayNameInput.val()), counter: g_displayNameRequestCounter}, function(json) {
        	if (json.counter != g_displayNameRequestCounter) {
                return;
            }
            if (json.error && json.error.length > 0) {
                // Has error
                displayNameError.show();
                displayNameError.text(json.error[0]);
                displayNameInput.addClass("inputError");
                profileURLBlock.hide();
            } else {
                // It's valid
                displayNameError.hide();
                displayNameInput.removeClass("inputError");

                                if (shortNameEdited) { 
                    return;
                }
                shortNameInput.val(json.shortName);
                                if(displayNameInput.val() && displayNameInput.val() != '') {
                	shortNameInput.val(json.shortName);
                    if(!shortNameEdition.is(":visible")) {
                    	profileURLBlock.show();
                    }
                } else {
                	shortNameInput.val('');
                }
                handleShortNameEdit(false);
            }
        });
    }
    
    var g_shortNameRequestCounter = 0;
    function handleShortNameEdit(edition) {
    	g_shortNameRequestCounter++;
        gjAjax.postRequest("/subscribe/ajax", {view: "json", action: "checkShortName", shortName: shortNameInput.val(), counter: g_shortNameRequestCounter}, function(json) {
        	if (json.counter != g_shortNameRequestCounter) {
        		return;
        	}
            if (json.error && json.error.length > 0) {
                // Has error
                shortNameError.show();
                shortNameError.text(json.error[0]);
                shortNameInput.addClass("inputError");
            } else {
                // It's valid
                shortNameError.hide();
                shortNameInput.removeClass("inputError");
                
                if (edition) {
                    var shortName = urlize(displayNameInput.val());
                    shortNameEdited = (shortName != shortNameInput.val()); 
                }
                if (shortNameInput.val() && shortNameInput.val() != '') {
                	profileURL.html("https://www.scoop.it/u/" + shortNameInput.val());
                }
            }
        });
    }
    
    function handlePasswordEdit() {
        if (passwordInput.val() == '') {
            // Has error
            passwordError.text('This information is required.');
            passwordError.show();
            passwordInput.addClass("inputError");
        } else {
            // It's valid
            passwordError.hide();
            passwordInput.removeClass("inputError");
        }
    }
    
    });
</script>

